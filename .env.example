GOOGLE_GENERATIVE_AI_API_KEY=your-google-api-key-here

# Slack Bot Configuration
# Get these values from your Slack App settings at https://api.slack.com/apps

# Bot User OAuth Token - starts with xoxb-
SLACK_BOT_TOKEN=xoxb-your-bot-token-here

# Channel ID where the bot will send messages
# Find this by right-clicking on a channel and selecting "Copy link"
# The ID is the last part after the last slash (e.g., C1234567890)
SLACK_CHANNEL_ID=xxx

# Signing Secret for verifying requests from Slack (optional but recommended)
# Found in your Slack App's "Basic Information" section
SLACK_SIGNING_SECRET=your-signing-secret-here

ATTIO_API_TOKEN=xxx