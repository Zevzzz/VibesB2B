{
  "name": "vibeb2b",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "mastra dev",
    "build": "mastra build",
    "start": "mastra start",
    "electron": "electron .",
    "electron-dev": "ELECTRON_IS_DEV=true electron .",
    "electron-pack": "electron-builder",
    "electron-dist": "electron-builder --dir",
    "build-css": "npx tailwindcss -i ./src/styles.css -o ./src/output.css --watch",
    "webhook-tunnel": "npx ngrok http 3001"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=20.9.0"
  },
  "build": {
    "appId": "com.vibeb2b.app",
    "productName": "VibeB2B",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/**/*",
      "!src/mastra/**/*",
      "!*.md",
      "!api/**/*",
      "!desktop-test-recordings/**/*",
      "!footage/**/*",
      "!out/**/*",
      "!recall-ai/**/*",
      "!*.tar.gz"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.17",
    "@mastra/core": "^0.18.0",
    "@mastra/libsql": "^0.14.3",
    "@mastra/loggers": "^0.10.13",
    "@mastra/memory": "^0.15.3",
    "@recallai/desktop-sdk": "^1.2.0",
    "@slack/web-api": "^7.10.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^24.5.2",
    "@vercel/node": "^5.3.24",
    "daisyui": "^5.1.25",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.0",
    "mastra": "^0.13.2",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}